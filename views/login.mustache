<!DOCTYPE html>
 <html lang="en">
   <head>
     <meta charset="utf-8">
     <title></title>
   </head>
   <body>
     <!-- sign up and sign in page redirect if requiremnets not met -->
     <div class="sign-up">
       <form class="" action="/signup" method="post">
         <input type="text" name="username" placeholder="Username">
         <input type="password" name="password" placeholder="Password">
         <input type="submit" name="" value="Sign Up">
       </form>
     </div>
     <div class="sign-in">
       <form class="" action="/signin" method="post">
         <input type="text" name="username" placeholder="Username">
         <input type="password" name="password" placeholder="Password">
         <input type="submit" name="" value="Sign In">
       </form>
     </div>
   </body>
 </html>
